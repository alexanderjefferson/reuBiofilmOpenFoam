FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue solid 0
);

regions
(
    cellToCell
    {
        set walls;

        fieldValues
        (
            volScalarFieldValue solid 1
        );
    }

);

